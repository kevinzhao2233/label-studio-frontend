    "key": "alt+space",
    "description": "播放/暂停"
  },
  "media:step-backward": {
    "key": "alt+left",
    "description": "回退一步"
  },
  "media:step-forward": {
    "key": "alt+right",
    "description": "向前一步"
  },
  "video:keyframe-backward": {
    "key": "ctrl+alt+left",
    "description": "上一个关键帧"
  },
  "video:keyframe-forward": {
    "key": "ctrl+alt+right",
    "description": "下一个关键帧"
  },
  "video:backward": {
    "key": "alt+left",
    "description": "返回"
  },
  "video:rewind": {
    "key": "shift+ctrl+alt+left",
    "description": "到第一帧"
  },
  "video:forward": {
    "key": "shift+alt+right",
    "description": "前进"
  },
  "video:fastforward": {
    "key": "shift+ctrl+alt+right",
    "description": "到最后一帧"
  },
  "video:hop-backward": {
    "key": "shift+alt+left",
    "description": "向后跳"
  },
  "video:hop-forward": {
    "key": "shift+alt+right",
    "description": "向前跳"
  }
}
