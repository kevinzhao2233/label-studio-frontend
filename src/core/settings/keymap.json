{
  "audio:back": {
    "key": "ctrl+b",
    "mac": "command+b",
    "description": "回退一秒"
  },
  "audio:playpause": {

  },
  "ts:grow-left":{
    "key": "left",
    "description": "向左增加标注区域"
  },
  "ts:grow-right":{
    "key": "right",
    "description": "向右增加标注区域"
  },
  "ts:shrink-left":{
    "key": "alt+left",
    "description": "减少左边的标注区域"
  },
  "ts:shrink-right":{
    "key": "alt+right",
    "description": "减少右边的标注区域"
  },
  "ts:grow-left-largre": {
    "key": "shift+left"
  },
  "ts:grow-right-largre": {
    "key": "shift+right"
  },
  "ts:shrink-left-largre": {
    "key": "shift+alt+left"
  },
  "ts:shrink-right-largre": {
    "key": "shift+alt+right"
  },
  "annotation:submit": {
    "key": "ctrl+enter",
    "mac": "command+enter",
    "description": "提交标注结果"
  },
  "annotation:skip": {
    "key": "ctrl+space",
    "mac": "alt+enter",
    "description": "跳过任务"
  },
  "annotation:undo": {
    "key": "ctrl+z",
    "mac": "command+z",
    "description": "撤销"
  },
  "annotation:redo": {
    "key": "ctrl+shift+z",
    "mac": "command+shift+z",
    "description": "重做"
  },
  "region:delete-all": {
    "key": "ctrl+backspace",
    "mac": "command+backspace",
    "description": "删除所有标注区域"
  },
  "region:focus": {
    "key": "enter",
    "description": "聚焦到第一个可聚焦的标注区域"
  },
  "region:relation": {
    "key": "alt+r",
    "description": "在标注区域之间创建关联"
  },
  "region:visibility": {
    "key": "alt+h",
    "description": "是否展示该标注区域"
  },
  "region:lock": {
    "key": "alt+l",
    "description": "Lock selected region"
  },
  "region:meta": {
    "key": "alt+m",
    "description": "Edit selected region meta"
  },
  "region:unselect": {
    "key": "u",
    "description": "取消选择"
  },
  "region:exit": {
    "key": "escape",
    "description": "退出关联模式，并取消选择"
  },
  "region:delete": {
    "key": "backspace",
    "description": "删除该标注区域"
  },
  "region:cycle": {
    "key": "alt+.",
    "description": "循环标注区域"
  },
  "region:duplicate": {
    "key": "ctrl+d",
    "mac": "command+d",
    "description": "复制选择的标注区域"
  },
